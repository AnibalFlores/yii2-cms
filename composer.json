{
    "name": "albertborsos/yii2-cms",
    "description": "CMS module for Yii2 Framework",
    "keywords": ["yii2", "cms", "extension"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/albertborsos/yii2-cms/issues",
        "source": "https://github.com/albertborsos/yii2-cms"
    },
    "authors": [
        {
            "name": "Albert Borsos",
            "email": "blog@borsosalbert.hu"
        }
    ],
	"respositories":[
		{
			"type":"vcs",
			"url":"git@github.com:albertborsos/yii2-fontawesome.git"
		}
	],
    "require": {
        "albertborsos/yii2-lib": "*",
        "albertborsos/yii2-tagger": "*",
        "albertborsos/yii2-user": "*",
        "albertborsos/yii2-historizer": "*",
        "albertborsos/yii2-fontawesome": "*",
        "kartik-v/yii2-nav-x": "*"
    },
    "autoload": {
        "psr-4": {
            "albertborsos\\yii2cms\\": ""
        }
    }
}